"""
Script for using sub
"""

import sys
from sub.main import download
from sub.main import parse_args

if __name__ == '__main__':
    try:
        options, dwn_this = parse_args()
        for this in dwn_this:
            download(this, options)
    except KeyboardInterrupt:
        sys.exit()
